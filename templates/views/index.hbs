<!DOCTYPE html>
<html>
    <head>
        <link rel="stylesheet" href="/css/layout.css">
    </head>
    <body>
        {{>header}}
         <div class="title">
        <h1>COVID-19</h1>
        <h2>World Data</h2>
        </div>
        <div id="globalInfo">
            <table border="1" align="center">
                <tr>
                    <th>NewConfirmed</th>
                    <th>TotalConfirmed</th>
                    <th>NewDeaths</th>
                    <th>TotalDeaths</th>
                    <th>NewRecovered</th>
                    <th>TotalRecovered</th>
                </tr>
                <tr id="global"></tr>
            </table>
        </div>
        <div id="notify"></div>
        <div class="title">
        <h3>Country Wise Data</h3>
        </div>
        <div id="country"></div>
        <div id="notifycountry"></div>
    </body>
        {{>footer}}
        <script src="/js/worldscript.js"></script>
        <script src="/js/countryscript.js"></script>
</html>